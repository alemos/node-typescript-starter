{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "Node-Typescript-Starter",
  "main": "dist/main.js",
  "scripts": {
    "serve:dev": "webpack --config webpack.config.dev.js --mode development",
    "serve:prod": "npm run build:prod && node ./dist/main.js",
    "build:prod": "webpack --config webpack.config.prod.js --mode production",
    "debug:code": "nodemon --inspect=7001 -e ts,tsx --exec node -r ts-node/register ./main.ts",
    "debug:chrome": "nodemon --inspect --inspect-brk -e ts,tsx --exec node -r ts-node/register ./main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alemos/node-typescript-starter.git"
  },
  "keywords": [],
  "author": "alemsa@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.11",
    "@types/webpack-env": "^1.16.0",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.2",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "path": "^0.12.7"
  },
  "bugs": {
    "url": "https://github.com/alemos/node-typescript-starter/issues"
  },
  "homepage": "https://github.com/alemos/node-typescript-starter#readme"
}
